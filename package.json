{
  "name": "demo-ng-openlayers",
  "version": "17.1.3",
  "scripts": {
    "ng": "ng",
    "start": "nx serve demo-ng-openlayers",
    "build:lib": "nx build ng-openlayers --configuration production",
    "copy:lib:docs": "cp -r *.md dist/libs/ng-openlayers",
    "build:demo": "nx build demo-ng-openlayers --base-href /ng-openlayers/",
    "build:gh-pages": "nx build --output-path docs --base-href /ng-openlayers/",
    "build": "npm run build:lib && npm run build:demo",
    "test": "npm run build:lib && nx test",
    "lint": "nx lint",
    "e2e": "npm run build:lib && nx e2e",
    "format": "prettier --write \"**/@(src|e2e)/**/*.ts\"",
    "release": "npm --cwd libs/ng-openlayers release"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^17.1.0",
